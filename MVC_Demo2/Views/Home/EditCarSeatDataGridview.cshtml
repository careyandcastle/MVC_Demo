@model MVC_Demo2.Models.MvcDemoModel.車位資料檔
@*@{
        Layout = null;
    }*@

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>EditCarSeatDataGridview</title>
</head>
<body>

    <h4>車位資料檔</h4>
    <hr />
    <div class="row">
        <div class="col-md-4">
            @*<form asp-action="EditCarSeatDataGridview">*@
            <form asp-action="EditCarSeatDataPost">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="事業" />
                <input type="hidden" asp-for="單位" />
                <input type="hidden" asp-for="部門" />
                <input type="hidden" asp-for="分部" />
                <input type="hidden" asp-for="建物編號" />
                <input type="hidden" asp-for="樓層" />
                <input type="hidden" asp-for="車位編號" />
                <div class="form-group">
                    <label asp-for="車位大小編號" class="control-label"></label>
                    <input asp-for="車位大小編號" class="form-control" />
                    <span asp-validation-for="車位大小編號" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="車位種類編號" class="control-label"></label>
                    <input asp-for="車位種類編號" class="form-control" />
                    <span asp-validation-for="車位種類編號" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="每月租金" class="control-label"></label>
                    <input asp-for="每月租金" class="form-control" />
                    <span asp-validation-for="每月租金" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="修改人" class="control-label"></label>
                    <input asp-for="修改人" class="form-control" />
                    <span asp-validation-for="修改人" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="修改時間" class="control-label"></label>
                    <input asp-for="修改時間" class="form-control" />
                    <span asp-validation-for="修改時間" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Save" class="btn btn-primary" />
                </div>
            </form>
        </div>
    </div>

    <div>
        <a asp-action="Index">Back to List</a>
    </div>

</body>
</html>
